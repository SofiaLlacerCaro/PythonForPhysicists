import numpy as np
def fizzbuzz(stop):
  empty_array = np.array([])
  x = 1
  while x <= stop:
    if x % 15 == 0:
      empty_array = np.append(empty_array, "Fizzbuzz")
    elif x % 3 == 0:
      empty_array = np.append(empty_array, "Fizz")
    elif x % 5 == 0:
      empty_array = np.append(empty_array, "Buzz")
    else:
      empty_array = np.append(empty_array, str(x))
    x += 1
  print(empty_array)
fizzbuzz(67)
